{
    "name": "adlurfm/yii2-widgets",
    "description": "Some widgets for Yii2",
    "type": "yii2-extension",
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.4",
        "yiisoft/yii2": "~2.0.0",
        "kartik-v/yii2-widget-datepicker": "@dev"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "adlurfm\\widgets\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Adlur Rahman",
            "email": "adlurfm@gmail.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
